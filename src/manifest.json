{
    "manifest_version": 2,
    "name": "snake for yt",
    "description": "replaces yt loading animation with og circle and adds snake game",
    "version": "1.0",
    "permissions": ["activeTab"],
    "content_scripts": [
        {
            "matches": ["https://*.youtube.com/*"],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        "game.js",
        "snake.js",
        "food.js",
        "grid.js",
        "input.js",
        "animation.js",
        "spinner.css",
        "main.js"
      ]
}
