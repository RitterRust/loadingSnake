{
    "name": "snake for yt",
    "description": "replaces yt loading animation with og circle and adds snake game",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": ["https://*.youtube.com/*"],
            "css": ["style.css"],
            "js": ["content-script.js"]
        }
    ]
  }
